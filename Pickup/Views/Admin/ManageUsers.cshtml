@using Pickup.Models.AdminViewModels
@model List<ViewUsersViewModel>


@inject SignInManager<ApplicationUser> SignInManager
@inject UserManager<ApplicationUser> UserManager
<h2>Manage Users </h2>
<table>

    @foreach (var user in Model)

    {
        @if (user.Username != UserManager.GetUserName(User))
        {
            <tr>
                <td>
                    @user.Username
                </td>
                <td><a asp-controller="Admin" asp-action="Deactivate" asp-route-userId="@user.UserId">Deactivate</a></td>

            </tr>
        }

    }
</table>