@using Pickup.Models.DonationPickupViewModels
@model FurniturePickupViewModel

<h2>Furniture Picked Up</h2>
<p>
    Enter the quantity next to the items being donated. Boxes must either be left blank or
    be a valid number.
</p>
@using (Html.BeginForm())
{
    @Html.HiddenFor(m => m.PickupID)
    <ul> @Html.EditorFor(m => m.FurnitureList)
    </ul>

    <button id="clear" type="submit">Add Furniture Picked Up</button>}
